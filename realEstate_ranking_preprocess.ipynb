        "# Load data\n",
        "ds = load_dataset(\"divaroffical/real_estate_ads\")\n",
        "df = ds['train'].to_pandas()\n",
        "print(\"Shape:\", df.shape)\n",
        "pd.set_option(\"display.max_columns\", None)\n",
        "df.head()"
      ]
