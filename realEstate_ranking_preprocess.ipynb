      "source": [
        "# Basic text cleaning\n",
        "normalizer = Normalizer()\n",
        "tokenizer = WordTokenizer()\n",
        "lemmatizer = Lemmatizer()\n",
        "stopwords = set(stopwords_list())"
      ]
    },
    {
      "cell_type": "code",
